  "repository": {
    "type": "git",
    "url": "https://github.com/codegenie-be/cg-aos.git"
  },
  "bugs": {
    "url": "https://github.com/codegenie-be/cg-aos/issues"
  },
  "homepage": "https://github.com/codegenie-be/cg-aos#readme",
  "author": "CODEGENIE <info@codegenie.be>",
  "license": "MIT",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/Codegenie-BE"
    }
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
